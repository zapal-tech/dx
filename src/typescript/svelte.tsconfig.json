{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Svelte",
  "extends": "./default.tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "module": "ESNext",
    "moduleResolution": "Bundler"
  }
}
